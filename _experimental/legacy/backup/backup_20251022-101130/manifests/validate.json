import json
import os

def validate_roles(file_path):
    with open(file_path, "r") as f:
        roles = json.load(f)
    for role in roles:
        assert "id" in role and "name" in role and "permissions" in role

def validate_versions(file_path):
    with open(file_path, "r") as f:
        data = json.load(f)
    assert "version" in data and "features" in data
